{
	"name": "frontend-boilerplate",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"start": "run-p start:dev \"codegen -- --watch\"",
		"start:dev": "node server.js",
		"codegen": "apollo client:codegen --customScalarsPrefix GraphQL --passthroughCustomScalars --target typescript --outputFlat src/lib/gqlQueryTypes",
		"generate-todos": "leasot -x --reporter markdown 'src/**/*.ts' '*.js' > TODO.md",
		"build": "next build",
		"export": "next export",
		"deploy": "npm run build && npm run export",
		"start:production": "NODE_ENV=production node server.js"
	},
	"dependencies": {
		"@apollo/react-hooks": "^3.1.3",
		"@apollo/react-ssr": "^3.1.3",
